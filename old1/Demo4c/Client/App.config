<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="rebus" type="Rebus.Configuration.RebusConfigurationSection, Rebus"/>
  </configSections>

  <rebus inputQueue="test.transports.client" errorQueue="error" workers="1" maxRetries="5">
    <endpoints>
      <add messages="Server.Messages" endpoint="test.transports.server"/>
    </endpoints>
  </rebus>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
